    "overall_data = []\n",
    "\n",
    "for p in paths:\n",
    "    with ThreadPoolExecutor(max_workers=5) as executor:\n",
    "        data = list(executor.map(extract_spam_assassin_features, [os.path.join(p, path) for path in os.listdir(p)]))\n",
    "\n",
    "    overall_data.extend(data)"
   ]
